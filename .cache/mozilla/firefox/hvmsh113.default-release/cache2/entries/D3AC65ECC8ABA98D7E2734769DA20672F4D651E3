  const messagePage = document.querySelector('#page-message-index #page-content');

  if (messagePage) {
    const backButton = messagePage.querySelector('[data-action="cancel-search"]');
    const dialogsBody = messagePage.querySelector('.i-dialogs-body');
    const search = messagePage.querySelector('.view-search');
    const mainSearch = messagePage.querySelector('.i-main-search');
    const mainSearchInput = mainSearch.querySelector('input');
    const emptySearchContainers = messagePage.querySelectorAll('[data-region="view-search"] [data-region="empty-message-container"]');
    const conversationBody = messagePage.querySelector('.view-conversation');

    backButton.addEventListener('click', () => {
      dialogsBody.classList.remove('hidden');
      search.classList.add('hidden');
      emptySearchContainers.forEach(container => {
        container.style.display = 'none';
      });
      mainSearch.classList.remove('hidden');
      conversationBody.classList.add('hidden');
    });

    mainSearchInput.addEventListener('click', () => {
      emptySearchContainers.forEach(container => {
        container.style.display = 'block';
      });
    });
  }
