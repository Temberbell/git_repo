  const modalExit = document.querySelector('.modal-exit');

  linkExit.href = buttonExit.href;

  const closeModalExit = document.querySelectorAll(
  'button[aria-label="Закрыть окно выхода"]'
  );

  closeModalExit.forEach((item) => {
  item.addEventListener("click", () => {
  modalExit.style.display = "none"
  });
  });

