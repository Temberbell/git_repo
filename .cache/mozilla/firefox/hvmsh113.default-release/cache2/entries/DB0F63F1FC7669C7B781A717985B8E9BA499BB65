require(['jquery', 'core/custom_interaction_events'], function($, CustomEvents) {
    CustomEvents.define('#single_select67d42b494ed0d6', [CustomEvents.events.accessibleChange]);
    $('#single_select67d42b494ed0d6').on(CustomEvents.events.accessibleChange, function() {
        var ignore = $(this).find(':selected').attr('data-ignore');
